{
  "name": "redbox/testsuite",
  "description": "Redbox-Testsuite allows you to batch process student like tests in a Suite of tests and collect the results.",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Johnny Mast",
      "email": "mastjohnny@gmail.com"
    }
  ],
  "require": {
    "php": ">= 7.4"
  },
  "autoload": {
    "psr-4": {
      "Redbox\\Testsuite\\": "src/",
      "Redbox\\Testsuite\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "phpspec/phpspec": "^7.0",
    "phpspec/prophecy": "^1.12",
    "squizlabs/php_codesniffer": "^3.5"
  }
}
